{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": ".",
        "version": "qsharp-preview",
        "dest": "api",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "versions": {
      "qsharp-preview": {
        "dest": "qsharp-preview"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "latex_macros": {
        "ket": ["\\left| #1\\right\\rangle", 1],
        "bra": ["\\left\\langle #1\\right|", 1],
        "braket": ["\\left\\langle #1 \\right\\rangle", 1],
        "boldone": "\\mathbf{1}",
        "expect": "\\mathbb{E}",
        "variance": "\\operatorname{Var}"
      }
    },
    "fileMetadata": {},
    "template": [],
    "markdownEngineName": "markdig",
    "dest": "quantum-api"
  }
}
